    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "ai": "^2.1.21",
    "class-variance-authority": "^0.6.1",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "common-tags": "^1.8.2",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "github-slugger": "^2.0.0",
    "gpt3-tokenizer": "^1.1.5",
    "lucide-react": "^0.260.0",
    "mdast": "^3.0.0",
